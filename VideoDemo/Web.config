      <!--name="VideoContext" 的部分， 一般會與先前建立的 DbContext 類別名稱相同（ 也可以用不同的名稱） 。 另一個需要留意的是 Initial Catalog=VideoDemo; 其中的 VideoDemo 代表所要建立的資料庫名稱。
          Data Source=(LocalDB)\v1 1 .0; 其中 (LocalDB)\v1 1 .0 代表資料庫伺服器名稱， 當安裝完 Visual Studio Community 201 3 之後， 內訂會同時幫我們建立一個名為 (LocalDB)\v1 1 .0 資料庫伺服器， 以供應用程式開發階段測試之用
      -->
      <add name="VideoContext" connectionString="Data Source=(LocalDb)\v11.0;Initial Catalog=VideoDemo;Integrated Security=True" providerName="System.Data.SqlClient" />
      <!--
  <add name="VideoContext" connectionString="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\VideoDemo.mdf;Initial Catalog=VideoDemo;Integrated Security=True" providerName="System.Data.SqlClient" />
  -->
